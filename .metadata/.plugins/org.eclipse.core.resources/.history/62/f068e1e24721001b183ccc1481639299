package wta_problem;

public class Weapon 
{
	private String name;
	private int ithName;
	
	
	/**
	 * @param name
	 * @param ithName
	 */
	public Weapon(String name, int ithName) {
		this.name = name;
		this.ithName = ithName;
	}
	
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @return the ithName
	 */
	public int getIthName() {
		return ithName;
	}
	/**
	 * @param ithName the ithName to set
	 */
	public void setIthName(int ithName) {
		this.ithName = ithName;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ithName;
		result = prime * result + ((name == null) ? 0 : name.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj) {
			return true;
		}
		if (obj == null) {
			return false;
		}
		if (!(obj instanceof Weapon)) {
			return false;
		}
		Weapon other = (Weapon) obj;
		/*if (ithName != other.ithName) {
			return false;
		}*/
		if (name == null) {
			if (other.name != null) {
				return false;
			}
		} else if (!name.equals(other.name)) {
			return false;
		}
		return true;
	}
	
	
	
	

}
