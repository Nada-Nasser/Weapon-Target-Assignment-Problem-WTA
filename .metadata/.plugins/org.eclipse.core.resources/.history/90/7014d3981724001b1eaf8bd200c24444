package wta_problem;

public class Gene 
{
	private int  target; // target name
	
	/**
	 * @param target : int
	 */
	public Gene(int target) {
		this.target = target;
	}
	
	/**
	 * @return the target
	 */
	public int getTarget() {
		return target;
	}
	/**
	 * @param target the target to set
	 */
	public void setTarget(int target) {
		this.target = target;
	}
	
}
