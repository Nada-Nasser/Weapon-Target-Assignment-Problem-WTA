package wta_problem;

public class Weapon 
{
	private String name;
	private int ithName;
	
	
	/**
	 * @param name
	 * @param ithName
	 */
	public Weapon(String name, int ithName) {
		this.name = name;
		this.ithName = ithName;
	}
	
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @return the ithName
	 */
	public int getIthName() {
		return ithName;
	}
	/**
	 * @param ithName the ithName to set
	 */
	public void setIthName(int ithName) {
		this.ithName = ithName;
	}
	
	

}
