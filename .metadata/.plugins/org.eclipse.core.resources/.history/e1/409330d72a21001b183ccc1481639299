package wta_problem;

public class Gene 
{
	private int  target;
	private String weapon;
	
	
	/**
	 * @return the target
	 */
	public int getTarget() {
		return target;
	}
	/**
	 * @param target the target to set
	 */
	public void setTarget(int target) {
		this.target = target;
	}
	/**
	 * @return the weapon
	 */
	public String getWeapon() {
		return weapon;
	}
	/**
	 * @param weapon the weapon to set
	 */
	public void setWeapon(String weapon) {
		this.weapon = weapon;
	}
	
	

}
