package wta_problem;

public class Target  //represents one bit in one gene.
{
	private float threat;
	private int name;
	
	/**
	 * @param threat
	 * @param name
	 */
	public Target( int name , float threat) {
		this.threat = threat;
		this.name = name;
	}
	/**
	 * @return the threat
	 */
	public float getThreat() {
		return threat;
	}
	/**
	 * @param threat the threat to set
	 */
	public void setThreat(float threat) {
		this.threat = threat;
	}
	/**
	 * @return the name
	 */
	public int getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(int name) {
		this.name = name;
	}
	
	
	
}
